<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <fragment        android:id="@+id/mapFragment"        android:name="com.google.android.gms.maps.SupportMapFragment"        android:layout_width="match_parent"        android:layout_height="match_parent"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        tools:context=".ui.map.MapFragment" />    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:id="@+id/add_fab"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_gravity="end"        android:layout_marginStart="16dp"        android:layout_marginBottom="16dp"        android:src="@drawable/add"        app:fabSize="normal"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent" />    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:id="@+id/setting_fab"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginBottom="24dp"        app:fabSize="normal"        app:layout_constraintBottom_toTopOf="@+id/add_fab"        app:layout_constraintEnd_toEndOf="@+id/add_fab"        app:layout_constraintStart_toStartOf="@+id/add_fab"        app:srcCompat="@drawable/setting" />    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:id="@+id/camera_fab"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginBottom="24dp"        app:fabSize="normal"        app:layout_constraintBottom_toTopOf="@+id/setting_fab"        app:layout_constraintEnd_toEndOf="@+id/setting_fab"        app:layout_constraintStart_toStartOf="@+id/setting_fab"        app:srcCompat="@drawable/camera" />    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:id="@+id/marker_fab"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginBottom="24dp"        app:fabSize="normal"        app:layout_constraintBottom_toTopOf="@+id/camera_fab"        app:layout_constraintEnd_toEndOf="@+id/camera_fab"        app:layout_constraintStart_toStartOf="@+id/camera_fab"        app:srcCompat="@drawable/marker" />    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:id="@+id/polyline_fab"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginBottom="24dp"        app:fabSize="normal"        app:layout_constraintBottom_toTopOf="@+id/marker_fab"        app:layout_constraintEnd_toEndOf="@+id/marker_fab"        app:layout_constraintStart_toStartOf="@+id/marker_fab"        app:srcCompat="@drawable/polyline" />    <com.google.android.material.floatingactionbutton.FloatingActionButton        android:id="@+id/clear_fab"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginBottom="24dp"        app:fabSize="normal"        app:layout_constraintBottom_toTopOf="@+id/polyline_fab"        app:layout_constraintEnd_toEndOf="@+id/polyline_fab"        app:layout_constraintStart_toStartOf="@+id/polyline_fab"        app:srcCompat="@drawable/clear" /></androidx.constraintlayout.widget.ConstraintLayout>